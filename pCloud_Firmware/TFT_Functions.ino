  else if ((TFT_WiFi_Strength >= 50) && (TFT_WiFi_Strength < 75)) {
    // tft.fillScreen(0x0000);
    tft.drawBitmap(5, 15, WiFi_75_Bitmap, 27, 15, 0x7740);

  }
  else if (TFT_WiFi_Strength >= 75) {
    //  tft.fillScreen(0x0000);
    tft.drawBitmap(5, 15, WiFi_100_Bitmap, 27, 15, 0x2F40);

  }
};

